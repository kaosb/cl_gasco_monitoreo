<div class="box">
	<div class="box-header with-border">
		<h3 class="box-title"><%=@action.name%></h3>
	</div>
	<div class="box-body">
		<%= line_chart @log.where('created_at > ?', 4.hours.ago).group_by_hour(:created_at).maximum(:response_time), discrete: true, {library: {hAxis: {title: "Hora"}, vAxis: {title: "Tiempo respuesta"}}} %>
	</div>
	<div class="box-footer">
	</div>
</div>
