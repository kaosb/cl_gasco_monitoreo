<div class="box">
	<div class="box-header with-border">
		<h3 class="box-title">Panorama de servicios</h3>
	</div>
	<div class="box-body">
			<%= pie_chart Log.group(:response_code).count %>
			<%= line_chart @logs.group(:action_id).group_by_hour(:created_at).group(:response_time).count, {library: {hAxis: {title: "Hora"}, vAxis: {title: "Tiempo respuesta"}}} %>
	</div>
	<div class="box-footer">
	</div>
</div>
